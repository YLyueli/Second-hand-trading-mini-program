Page {
  margin-bottom: 130rpx;
  position: relative;
}
.search_input {
  height: 120rpx;
  padding: 0rpx 30rpx 0rpx 30rpx;
  background-color: var(--themeColor);
  margin: 0 0 20rpx 0;
  display: flex;
  align-items: center;
  width: 100%;
  justify-content: center;
  position: fixed;
  top: 0;
  left: 0;
}
.search_input navigator {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-left: 40rpx;
  height: 60%;
  background-color: #fff;
  border-radius: 60rpx;
  color: #666;
}
.search_btn {
  width: 160rpx;
  text-align: center;
}
.search_input .iconfont {
  color: var(--themeColor);
  font-size: 45rpx;
  margin-right: 20rpx;
}

.classify_wrap {
  display: flex;
  align-items: center;
  background-color: #fff;
  margin-bottom: 20rpx;
  flex-wrap: nowrap;
  height: 100rpx;
}
scroll-view {
  height: 40rpx;
  white-space: nowrap;
}
.classify {
  display: inline-block;
  width: 120rpx;
  padding: 30rpx;
}

.active {
  color: var(--themeColor);
  font-weight: 600;
}

.index_business_display {
  width: 720rpx;
  /* column-count: 2;
  column-width: 340rpx;
  column-gap: 20rpx; */
  margin: 0 auto;
  display: flex;
  align-items: center;
  justify-content: space-between;
  flex-wrap: wrap;
  margin-top: 140rpx;
}
.index_business_display .index_business_wrap {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  background-color: #fff;
  border-radius: 30rpx;
  overflow: hidden;
  margin-bottom: 20rpx;
  break-inside: avoid;
  width: 350rpx;
}
.index_business_display .index_business_wrap image {
  width: 100% !important;
  max-height: 370rpx !important;
}
.index_business_display .index_business_wrap .business_title {
  font-size: 28rpx;
  font-weight: 600;
  padding: 10rpx 0 0 20rpx;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.index_business_display .index_business_wrap .business_price_wrap {
  padding: 10rpx 20rpx 0 20rpx ;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  color: #999595;
  font-size: 24rpx;
}
.index_business_display .index_business_wrap .business_price_wrap .business_price {
  font-size: 28rpx;
  color: #F74C56;
  font-weight: 600;
  margin-right: 30rpx;
}
.index_business_display .index_business_wrap .business_school {
  padding-left: 20rpx;
  margin: 10rpx 0;
}
.index_business_display .index_business_wrap .business_school text {
  padding: 5rpx 10rpx;
  border: 1rpx solid var(--themeColor);
  border-radius: 20rpx;
  color: var(--themeColor);
  font-size: 24rpx;
}
.index_business_display .index_business_wrap .business_seller {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 10rpx 0 20rpx 20rpx;
  color: #999595;
  font-size: 26rpx;
}
.index_business_display .index_business_wrap .business_seller .business_seller_icon {
  width: 40rpx !important;
  height: 40rpx !important;
  border-radius: 50%;
  /* border: 1rpx solid var(--themeColor); */
}
.index_business_display .index_business_wrap .business_seller .business_seller_name {
  font-weight: 500;
  padding-left: 10rpx;
}
.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_cate {
  display: flex;
}
.index_cate navigator {
  flex: 1;
  padding: 20rpx;
}
.index_cate navigator image {
  width: 100%;
}
.index_floor .floor_group .floor_title image {
  width: 100%;
}
.index_floor .floor_group .floor_list {
  overflow: hidden;
  width: 100%;
}
.index_floor .floor_group .floor_list .floor_style {
  float: left;
  width: 33.33%;
}
.index_floor .floor_group .floor_list .floor_style image {
  width: 100%;
  height: 100%;
}
.index_floor .floor_group .floor_list .floor1,
.index_floor .floor_group .floor_list .floor2,
.index_floor .floor_group .floor_list .floor3,
.index_floor .floor_group .floor_list .floor4 {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.index_floor .floor_group .floor_list .floor1,
.index_floor .floor_group .floor_list .floor2 {
  border-bottom: 10rpx solid #fff;
}
.no_business {
  width: 100%;
  height: 100vh;
  text-align: center;
  line-height: 100vh;
  font-size: 30rpx;
  color: #666;
  font-weight: 600;
}