Page {
  background-color: #fff;
}
.login_wrap {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.login_wrap .logo {
  margin: 60rpx 0 40rpx 0;
}
.login_wrap .logo image {
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
  padding: 10rpx;
  border: 1rpx solid var(--themeColor);
}
.login_wrap .welcome {
  font-size: 30rpx;
  font-weight: 600;
  color: #333;
  padding: 10rpx;
}
.login_wrap .login_btn {
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 120rpx 0;
  width: 70%;
  background-color: var(--themeColor);
  color: #fff;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 60rpx;
  text-align: center;
  font-size: 36rpx;
}
.login_wrap .login_btn .iconfont {
  font-size: 40rpx;
  padding-right: 20rpx;
}
